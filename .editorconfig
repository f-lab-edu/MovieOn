# top-most EditorConfig file
root = true

[*]
# [encoding-utf8]
charset = utf-8

# [newline-eof]
insert_final_newline = true

# Ignore
[*.log]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset

[*.bat]
end_of_line = crlf

[*.java]
# [indentation-space]
indent_style = space

# [4-spaces-tab]
indent_size = 4
tab_width = 4

# [no-trailing-spaces]
trim_trailing_whitespace = true

# [line-length-100]
max_line_length = 100